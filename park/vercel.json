{
  "builds": [
    {
      "src": "api/src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "park/vite.config.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "park/dist/$1"
    }
  ],
  "buildCommand": "cd park && npm install && npm run build",
  "outputDirectory": "park/dist"
}
